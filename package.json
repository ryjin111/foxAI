{
  "name": "foxai-elizaos",
  "version": "1.0.0",
  "description": "Custom ElizaOS implementation for FoxAI - Autonomous AI Agent",
  "main": "src/index.ts",
  "scripts": {
    "dev": "concurrently \"npm run server:dev\" \"npm run dashboard:dev\"",
    "server:dev": "ts-node src/index.ts",
    "dashboard:dev": "next dev",
    "build": "npm run build:server && npm run build:dashboard",
    "build:server": "tsc",
    "build:dashboard": "next build",
    "start": "node dist/index.js",
    "test": "jest",
    "eliza:dev": "ts-node src/elizaos/core.ts",
    "eliza:start": "node dist/elizaos/core.js",
    "vercel-build": "npm run build:dashboard"
  },
  "keywords": [
    "elizaos",
    "autonomous-agent",
    "ai",
    "automation",
    "twitter",
    "crypto",
    "hyperliquid",
    "nextjs",
    "dashboard"
  ],
  "author": "FoxAI",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "node-cron": "^3.0.3",
    "twitter-api-v2": "^1.15.2",
    "axios": "^1.6.0",
    "winston": "^3.11.0",
    "uuid": "^9.0.1",
    "lodash": "^4.17.21",
    "next": "^14.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.3.0",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.31",
    "@heroicons/react": "^2.0.18",
    "recharts": "^2.8.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/node": "^20.8.0",
    "@types/node-cron": "^3.0.11",
    "@types/uuid": "^9.0.7",
    "@types/lodash": "^4.14.202",
    "typescript": "^5.2.2",
    "ts-node": "^10.9.1",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.8",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "concurrently": "^8.2.2"
  }
} 